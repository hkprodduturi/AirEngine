{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://airengine.dev/schemas/online-eval-result.schema.json",
  "title": "OnlineEvalResult",
  "description": "Schema for AirEngine online eval harness results.",
  "type": "object",
  "required": [
    "schema_version",
    "timestamp",
    "run_metadata",
    "config",
    "corpus",
    "cases",
    "metrics",
    "artifact_dir"
  ],
  "properties": {
    "schema_version": { "const": "1.0" },
    "timestamp": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}T" },
    "run_metadata": { "$ref": "#/$defs/runMetadata" },
    "config": { "$ref": "#/$defs/evalConfig" },
    "corpus": { "$ref": "#/$defs/corpusInfo" },
    "cases": {
      "type": "array",
      "items": { "$ref": "#/$defs/caseResult" }
    },
    "metrics": { "$ref": "#/$defs/aggregateMetrics" },
    "artifact_dir": { "type": "string" }
  },
  "additionalProperties": false,
  "$defs": {
    "runMetadata": {
      "type": "object",
      "required": ["airengine_version", "node_version", "platform"],
      "properties": {
        "airengine_version": { "type": "string" },
        "node_version": { "type": "string" },
        "platform": { "type": "string" },
        "git_commit": { "type": "string" }
      },
      "additionalProperties": false
    },
    "evalConfig": {
      "type": "object",
      "required": [
        "generator_model",
        "repair_mode",
        "max_repair_attempts",
        "timeout_ms",
        "provider_retries"
      ],
      "properties": {
        "generator_model": { "type": "string" },
        "repair_mode": { "type": "string", "enum": ["deterministic", "claude", "none"] },
        "max_repair_attempts": { "type": "number", "minimum": 1 },
        "timeout_ms": { "type": "number", "minimum": 0 },
        "provider_retries": { "type": "number", "minimum": 0 },
        "repair_model": { "type": "string" },
        "repair_provider_retries": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "corpusInfo": {
      "type": "object",
      "required": ["path", "total_entries", "limit_applied", "corpus_id"],
      "properties": {
        "path": { "type": "string" },
        "total_entries": { "type": "number", "minimum": 0 },
        "limit_applied": {
          "oneOf": [
            { "type": "number", "minimum": 1 },
            { "type": "null" }
          ]
        },
        "corpus_id": { "type": "string" }
      },
      "additionalProperties": false
    },
    "outcomeCategory": {
      "type": "string",
      "enum": [
        "success_running_app",
        "generation_failed_auth",
        "generation_failed_provider",
        "generation_failed_invalid_air",
        "loop_failed_validation",
        "loop_failed_repair",
        "loop_failed_transpile",
        "loop_failed_smoke",
        "loop_failed_determinism",
        "unexpected_error"
      ]
    },
    "generationInfo": {
      "type": "object",
      "required": ["success", "duration_ms"],
      "properties": {
        "success": { "type": "boolean" },
        "duration_ms": { "type": "number", "minimum": 0 },
        "model": { "type": "string" },
        "attempts": { "type": "number", "minimum": 0 },
        "input_tokens": { "type": "number", "minimum": 0 },
        "output_tokens": { "type": "number", "minimum": 0 },
        "error": { "type": "string" }
      },
      "additionalProperties": false
    },
    "loopStage": {
      "type": "object",
      "required": ["name", "status", "duration_ms"],
      "properties": {
        "name": { "type": "string" },
        "status": { "type": "string", "enum": ["pass", "fail", "skip"] },
        "duration_ms": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "loopInfo": {
      "type": "object",
      "required": ["stages"],
      "properties": {
        "stages": {
          "type": "array",
          "items": { "$ref": "#/$defs/loopStage" }
        },
        "repair_attempts": { "type": "number", "minimum": 0 },
        "deterministic": { "type": "boolean" },
        "file_count": { "type": "number", "minimum": 0 },
        "output_lines": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "caseResult": {
      "type": "object",
      "required": ["id", "prompt_hash", "outcome", "generation", "total_duration_ms"],
      "properties": {
        "id": { "type": "string" },
        "prompt_hash": { "type": "string" },
        "outcome": { "$ref": "#/$defs/outcomeCategory" },
        "generation": { "$ref": "#/$defs/generationInfo" },
        "loop": { "$ref": "#/$defs/loopInfo" },
        "total_duration_ms": { "type": "number", "minimum": 0 },
        "error": { "type": "string" }
      },
      "additionalProperties": false
    },
    "timingMetrics": {
      "type": "object",
      "required": [
        "avg_total_ms",
        "p50_total_ms",
        "p95_total_ms",
        "avg_generation_ms",
        "avg_loop_ms"
      ],
      "properties": {
        "avg_total_ms": { "type": "number", "minimum": 0 },
        "p50_total_ms": { "type": "number", "minimum": 0 },
        "p95_total_ms": { "type": "number", "minimum": 0 },
        "avg_generation_ms": { "type": "number", "minimum": 0 },
        "avg_loop_ms": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "tokenMetrics": {
      "type": "object",
      "required": ["total_input", "total_output", "avg_input", "avg_output"],
      "properties": {
        "total_input": { "type": "number", "minimum": 0 },
        "total_output": { "type": "number", "minimum": 0 },
        "avg_input": { "type": "number", "minimum": 0 },
        "avg_output": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "retryMetrics": {
      "type": "object",
      "required": ["avg_generation_attempts", "avg_repair_attempts"],
      "properties": {
        "avg_generation_attempts": { "type": "number", "minimum": 0 },
        "avg_repair_attempts": { "type": "number", "minimum": 0 }
      },
      "additionalProperties": false
    },
    "failureBreakdown": {
      "type": "object",
      "properties": {
        "success_running_app": { "type": "number" },
        "generation_failed_auth": { "type": "number" },
        "generation_failed_provider": { "type": "number" },
        "generation_failed_invalid_air": { "type": "number" },
        "loop_failed_validation": { "type": "number" },
        "loop_failed_repair": { "type": "number" },
        "loop_failed_transpile": { "type": "number" },
        "loop_failed_smoke": { "type": "number" },
        "loop_failed_determinism": { "type": "number" },
        "unexpected_error": { "type": "number" }
      },
      "additionalProperties": false
    },
    "aggregateMetrics": {
      "type": "object",
      "required": [
        "total_cases",
        "completed_cases",
        "success_count",
        "prompt_to_air_success_rate",
        "prompt_to_running_app_success_rate",
        "timing",
        "tokens",
        "retries",
        "failure_breakdown"
      ],
      "properties": {
        "total_cases": { "type": "number", "minimum": 0 },
        "completed_cases": { "type": "number", "minimum": 0 },
        "success_count": { "type": "number", "minimum": 0 },
        "prompt_to_air_success_rate": { "type": "number", "minimum": 0 },
        "prompt_to_running_app_success_rate": { "type": "number", "minimum": 0 },
        "timing": { "$ref": "#/$defs/timingMetrics" },
        "tokens": { "$ref": "#/$defs/tokenMetrics" },
        "retries": { "$ref": "#/$defs/retryMetrics" },
        "failure_breakdown": { "$ref": "#/$defs/failureBreakdown" }
      },
      "additionalProperties": false
    }
  }
}
