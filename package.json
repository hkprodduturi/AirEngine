{
  "name": "airengine",
  "version": "0.2.0",
  "description": "AI-native Intermediate Representation â€” one .air file, full-stack apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "air": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "docs",
    "examples"
  ],
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:complex-baseline": "vitest run tests/complex-app-baseline.test.ts",
    "test:complex-gaps": "vitest run --config vitest.config.gaps.ts",
    "lint": "eslint src/",
    "generate": "node dist/cli/index.js generate",
    "transpile": "node dist/cli/index.js transpile",
    "validate": "node dist/cli/index.js validate",
    "mcp": "npx tsx src/mcp/server.ts",
    "build:gallery": "tsx scripts/build-gallery.ts",
    "eval-local": "tsx scripts/eval-local.ts",
    "foundation-check": "node --import tsx scripts/foundation-check.ts",
    "benchmark-compare": "node --import tsx scripts/benchmark-compare.ts",
    "demo-canonical": "node --import tsx scripts/demo-canonical.ts",
    "demo-prompt-replay": "node --import tsx scripts/demo-prompt-replay.ts",
    "eval-online": "node --import tsx scripts/eval-online.ts",
    "eval-online-compare": "node --import tsx scripts/eval-online-compare.ts",
    "quality-gate": "node --import tsx scripts/quality-gate.ts",
    "demo-live-canonical": "node --import tsx scripts/demo-live-canonical.ts",
    "doctor": "node --import tsx scripts/doctor.ts",
    "release-rehearsal": "node --import tsx scripts/release-alpha-rehearsal.ts",
    "stability-sweep": "node --import tsx scripts/stability-sweep.ts",
    "helpdesk-golden": "node --import tsx scripts/helpdesk-golden-run.ts",
    "photography-golden": "node --import tsx scripts/photography-premium-golden-run.ts",
    "eval-complex": "node --import tsx scripts/eval-complex.ts",
    "complex-readiness": "npm run test:complex-baseline && npm run test:complex-gaps && npm run helpdesk-golden"
  },
  "keywords": [
    "ai",
    "compiler",
    "transpiler",
    "code-generation",
    "full-stack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hkprodduturi/AirEngine.git"
  },
  "homepage": "https://github.com/hkprodduturi/AirEngine#readme",
  "bugs": {
    "url": "https://github.com/hkprodduturi/AirEngine/issues"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "zod": "^3.23.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "tsx": "^4.19.0",
    "vitest": "^1.6.0",
    "@types/node": "^20.12.0",
    "eslint": "^9.0.0"
  }
}
